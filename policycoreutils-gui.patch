+		for i in self.dirs.keys():
+			newsh += re.sub("FILENAME", i, script.restorecon)
+
+                for i in self.in_tcp[PORTS] + self.out_tcp[PORTS]:
+                    if self.find_port(i,"tcp") == None:
+                        t1 = re.sub("PORTNUM", "%d" % i, script.tcp_ports)
