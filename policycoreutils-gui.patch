+
+                for i in self.in_udp[PORTS] + self.out_udp[PORTS]:
+                    if self.find_port(i,"udp") == None:
+			t1 = re.sub("PORTNUM", "%d" % i, script.udp_ports)
+			newsh += re.sub("TEMPLATETYPE", self.name, t1)
+
+                newsh += self.generate_user_sh()
+			
+		return newsh
